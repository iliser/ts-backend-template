{
  "name": "fastify-typescript-starter",
  "version": "3.0.0",
  "description": "Fastify TypeScript starter repository",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "debug": "nodemon --config nodemon-debug.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "prod": "cross-env NODE_ENV=production node dist/server.js"
  },
  "author": "iliser <i.d.mvmii@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "fastify": "^2.12.1",
    "fastify-cors": "^3.0.3",
    "fastify-multipart": "^1.0.5",
    "fastify-static": "^2.7.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.5",
    "mongoose": "^5.9.6",
    "mongoose-deep-populate": "^3.1.1",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.7",
    "@types/node": "^13.9.1",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
